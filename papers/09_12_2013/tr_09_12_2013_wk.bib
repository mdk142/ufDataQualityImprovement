@article{ ISI:000323301600001,
Author = {Choi, Hee Min and Hobert, James P.},
Title = {{The Polya-Gamma Gibbs sampler for Bayesian logistic regression is
   uniformly ergodic}},
Journal = {{ELECTRONIC JOURNAL OF STATISTICS}},
Year = {{2013}},
Volume = {{7}},
Pages = {{2054-2064}},
Abstract = {{One of the most widely used data augmentation algorithms is Albert and
   Chibs (1993) algorithm for Bayesian probit regression. Polson, Scott,
   and Windle (2013) recently introduced an analogous algorithm for
   Bayesian logistic regression. The main difference between the two is
   that Albert and Chibs (1993) truncated normals are replaced by so-called
   Polya-Gamma random variables. In this note, we establish that the Markov
   chain underlying Polson, Scott, and Windles (2013) algorithm is
   uniformly ergodic. This theoretical result has important practical
   benefits. In particular, it guarantees the existence of central limit
   theorems that can be used to make an informed decision about how long
   the simulation should be run.}},
