
@article{ ISI:000323364400005,
Author = {Fox, Kathleen A. and Ward, Jeffrey T. and Lane, Jodi},
Title = {{Selection for Some, Facilitation for Others? Self-Control Theory and the
   Gang-Violence Relationship}},
Journal = {{DEVIANT BEHAVIOR}},
Year = {{2013}},
Volume = {{34}},
Number = {{12}},
Pages = {{996-1019}},
Month = {{DEC 1}},
Abstract = {{Although self-control theory has been thought to be entirely consistent
   with the gang selection model, key theoretical predictions of the
   general theory imply gang selection effects for those with lower
   self-control and gang facilitation effects for those with higher
   self-control. This new hypothesis is tested among a large sample of jail
   inmates. Results indicate that self-control did not render the
   gang-violence relationship spurious for the sample as a whole. Gang
   membership had a significantly greater impact on violent crime among
   those with very high self-control, but there were still statistically
   significant gang facilitation effects for the other three self-control
   groups.}},
